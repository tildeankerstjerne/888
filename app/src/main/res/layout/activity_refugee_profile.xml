<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".RefugeeProfile">


    <Button
        android:id="@+id/button_edit_refugee_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="161dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="3dp"
        android:text="@string/edit_refugee_notes"
        app:layout_constraintBottom_toTopOf="@+id/text_speaklanguage_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_role_profile" />

    <TextView
        android:id="@+id/text_username_profile"
        android:layout_width="132dp"
        android:layout_height="44dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="10dp"
        android:text="@string/username_refugeeprofile"
        android:textColor="#D66850"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/text_role_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_backarrow_profile"
        app:layout_constraintTop_toBottomOf="@+id/picture_profileicon_profile" />

    <Button
        android:id="@+id/button_back_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="302dp"
        android:text="@string/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/text_role_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="162dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="14dp"
        android:text="@string/role_refugee_refugeeprofile"
        android:textColor="#6246EA"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button_edit_refugee_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_username_profile" />

    <TextView
        android:id="@+id/text_speaklanguage_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="1dp"
        android:text="@string/speaklanguage_refugeeprofile"
        android:textColor="#6246EA"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_edit_refugee_profile" />

    <TextView
        android:id="@+id/text_applanguage_profile"
        android:layout_width="223dp"
        android:layout_height="28dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="144dp"
        android:text="@string/applanguage_refugeeprofile"
        android:textColor="#6246EA"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/text_selected_applanguage_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />


    <TextView
        android:id="@+id/text_selected_applanguage_profile"
        android:layout_width="110dp"
        android:layout_height="27dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="257dp"
        android:layout_marginBottom="266dp"
        android:text="@string/english_refugee_profile"
        android:textColor="#484071"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_applanguage_profile" />

    <ImageView
        android:id="@+id/picture_profileicon_profile"
        android:layout_width="77dp"
        android:layout_height="73dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="11dp"
        android:src="@drawable/baseline_person_24"
        app:layout_constraintBottom_toTopOf="@+id/text_username_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_back_profile"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/button_backarrow_profile"
        android:layout_width="74dp"
        android:layout_height="71dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="3dp"
        android:contentDescription="@string/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_back_profile"
        app:srcCompat="@drawable/back_arrow" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="283dp"
        android:layout_height="71dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="2dp"
        android:background="@color/behind_text"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/text_applanguage_profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_speaklanguage_profile">

        <TextView
            android:id="@+id/text_selected_speaklanguage_eng_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/english_refugee_profile"
            android:textColor="#484071"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="109dp"
            tools:layout_editor_absoluteY="354dp" />

        <TextView
            android:id="@+id/text_selected_speaklanguage_arb_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/arabic_refugee_profile"
            android:textColor="#484071"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="109dp"
            tools:layout_editor_absoluteY="354dp" />

        <TextView
            android:id="@+id/text_selected_speaklanguage_dan_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/dansk_refugee_profile"
            android:textColor="#484071"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="109dp"
            tools:layout_editor_absoluteY="354dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>